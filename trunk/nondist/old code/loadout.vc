//worlds other most boring map
//does the load out menu
void startup()
{
	while(!nLoadMenuDone)
	{
		Render();
		ShowPage();
		
		if(nLoadMenuChoice >= 0)
		{
			//player chose the same weapon, remove it
			if(nLoadMenuChoice == nLoadMenuWeap)
				nLoadMenuWeap = 0-1;
			else //else set the weapon that we chose
			{
				nLoadMenuWeap = nLoadMenuChoice;
			}
			
			nLoadMenuChoice = 0-1;
		}
		
		if(nLoadMenuDone)
		{
			NewGame();
			map(sLoadMenuNextMap);
		}
	}
}
